  "main": "index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "build": "mkdir -p dist/src/webserver && cp -R ./src/webserver/public ./dist/src/webserver/public && tsc --project tsconfig.json",
    "dev": "nodemon --config nodemon.json",
