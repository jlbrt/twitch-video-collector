    "db:clear": "node dist/scripts/clearDB",
    "db:migrate:dev": "ts-node scripts/migrateDB.ts",
    "db:rollback:dev": "ts-node scripts/rollbackDB.ts",
    "lint": "prettier --check ./src ./scripts ./migrations"
  },
